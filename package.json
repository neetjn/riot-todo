{
  "name": "mock-site",
  "version": "1.0.0",
  "description": "Mock site created with riot.js for testing py-component-controller.",
  "main": "index.js",
  "scripts": {
    "setup": "npm install && npm run build",
    "build": "node_modules/.bin/cross-env NODE_ENV=production NODE_ENV=production node_modules/.bin/webpack --config build/webpack.conf.js",
    "build:dev": "node_modules/.bin/webpack --config build/webpack.conf.js",
    "app": "node_modules/.bin/webpack-dev-server --config build/webpack.conf.js --port 3000",
    "app:detached": "nohup npm run app &",
    "lint": "node_modules/.bin/eslint src/**.js",
    "test": "npm run lint"
  },
  "author": "John Nolette",
  "license": "MIT",
  "dependencies": {
    "riot": "3.7.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.1",
    "cross-env": "5.0.1",
    "css-loader": "0.28.0",
    "eslint": "4.1.1",
    "eslint-loader": "1.6.1",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.2",
    "node-sass": "4.0.0",
    "raw-loader": "0.5.1",
    "riot-tag-loader": "1.0.0",
    "sass-loader": "6.0.5",
    "style-loader": "0.18.0",
    "url-loader": "0.5.9",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.9.4",
    "webpack-hot-middleware": "^2.20.0"
  }
}
